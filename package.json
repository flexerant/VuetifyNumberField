{
  "name": "flexerant-vuetify-number-field",
  "version": "1.1.4",
  "description": "A vuetify based numeric field component.",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts --skipLibCheck",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "test:unit": "cross-env NODE_ICU_DATA=node_modules/full-icu vue-cli-service test:unit",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/v-number-field.ssr.js",
  "module": "dist/v-number-field.esm.js",
  "browser": "dist/v-number-field.esm.js",
  "unpkg": "dist/v-number-field.min.js",
  "files": [
    "dist/*",
    "v-number-field.d.ts",
    "src/**/*.*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/jest": "^24.0.19",
    "@types/numeral": "0.0.29",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "^1.0.3",
    "cross-env": "^7.0.2",
    "full-icu": "^1.3.1",
    "material-design-icons-iconfont": "^6.1.0",
    "minimist": "^1.2.5",
    "node-sass": "^7.0.0",
    "numeral": "^2.0.6",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^10.1.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.4.2",
    "vuetify-loader": "^1.3.0"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vuetify": "^2.2.11",
    "numeral": "^2.0.6"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/flexerant/VuetifyNumberField",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexerant/VuetifyNumberField.git"
  },
  "types": "v-number-field.d.ts"
}
